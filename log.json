{
  "version": 1,
  "formatters": {
    "formatter": {
      "format": "%(asctime)s.%(msecs)03d Â· %(levelname)s: %(message)s",
      "datefmt": "%Y-%m-%d %H:%M:%S"
    }
  },
  "handlers": {
    "console": {
      "class": "logging.StreamHandler",
      "formatter": "formatter",
      "level": "DEBUG",
      "stream": "ext://sys.stdout"
    }
  },
  "loggers": {
    "logger": {
      "level": "DEBUG",
      "handlers": ["console"]
    }
  }
}
